module converter (
    input pp[16],
    input lives[16],
    input level[16],
    output out[4][4]
  ) {
  sig diff[5];
  sig diff2[5];
  sig diff3[16];
  sig diff4[16];
  always {
    diff = (pp[6:0]-((pp[6:0]/b01010)*b01010));
    out[0] = diff[3:0];
    diff2 = (pp[6:0]/b01010);
    out[1] = diff2[3:0];
    diff3 = lives;
    out[2] = diff3[3:0];
    diff4 = level;
    out[3] = b0011; 
  }
}